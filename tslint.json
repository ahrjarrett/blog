{
  "defaultSeverity": "error",
  // "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "no-console": false,
    "member-access": false,
    "object-literal-sort-keys": true,
    "ordered-imports": false,
    "interface-name": false,
    "no-submodule-imports": false,
    "quotemark": [true, "double", "jsx-double"],
    "arrow-parens": false
  },
  "rulesDirectory": [],
  "semicolon": [true, "always"]
}
